<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Data awal untuk sequence -->
    <record id="seq_type_icofr_control" model="ir.sequence">
        <field name="name">Icofr Control</field>
        <field name="code">icofr.control</field>
        <field name="prefix">CTRL/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

